ion-list.talk-list
  a.item.item-avatar(ng-repeat='subscribe in subscribes track by $index', type='item-text-wrap',  ng-click="Api.jumpTo(subscribe.clip, 'chats')")
    img(ng-src='{{subscribe["@clip"].gameData.logo?subscribe["@clip"].gameData.logo:""}}')
    i.icon.ion-ios-circle-filled.color-r(ng-if="subscribe.unread")
    b {{subscribe['@clip'].lastCommentUserData.email}}
    | {{subscribe["@clip"].content}} {{subscribe._modified | timeBefore}} {{subscribe["@clip"]}}
    span.item-note(ng-class='{first:$first}')
      img(ng-src="{{subscribe['@clip'].img['100']}}")

