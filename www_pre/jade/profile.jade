ion-view.full-card(view-title='个人')
  ion-nav-buttons(side="right")
    a.button.icon.ion-ios-gear-outline.button-clear.color-w(href='#/tab/profile-feels')
  ion-content
    .list.card.top-card
      a.item.item-avatar.item-icon-right
        img(src='img/mcfly.jpg')
        h2 {{userData.email}}
        p Game: {{channel.gameData.name}}
    .list.card(ng-repeat='clip in clips')
      .item.item-divider
        | [圖標] 刀塔傳奇 - {{clip.content}}
      .item.item-body
        a(href='#/tab/profile-chat/{{clip._id}}')
          img.full-image(src="{{clip.img}}")

